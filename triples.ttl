# ========== ONTOLOGY DEFINITIONS ==========

# Core Classes
ex:Project a owl:Class ;
    rdfs:label "Project" .

ex:Case a owl:Class ;
    rdfs:label "Project Case" .

ex:CargoItem a owl:Class ;
    rdfs:label "Cargo Item" .

ex:TransportEvent a owl:Class ;
    rdfs:label "Transport Event" .

ex:StockSnapshot a owl:Class ;
    rdfs:label "Stock Snapshot" .

ex:Invoice a owl:Class ;
    rdfs:label "Invoice" .

ex:BillOfLading a owl:Class ;
    rdfs:label "Bill of Lading" .

ex:HSCode a owl:Class ;
    rdfs:label "HS Code" .

# Properties
ex:caseNumber a owl:DatatypeProperty ;
    rdfs:domain ex:Case ;
    rdfs:range xsd:string .

ex:belongsToCase a owl:ObjectProperty ;
    rdfs:range ex:Case .

ex:eventDateTime a owl:DatatypeProperty ;
    rdfs:range xsd:dateTime .

ex:eventType a owl:DatatypeProperty ;
    rdfs:range xsd:string .

ex:sourceLocation a owl:DatatypeProperty ;
    rdfs:range xsd:string .

ex:targetLocation a owl:DatatypeProperty ;
    rdfs:range xsd:string .

ex:snapshotDate a owl:DatatypeProperty ;
    rdfs:range xsd:date .

ex:snapshotLocation a owl:DatatypeProperty ;
    rdfs:range xsd:string .

ex:inboundCount a owl:DatatypeProperty ;
    rdfs:range xsd:decimal .

ex:outboundCount a owl:DatatypeProperty ;
    rdfs:range xsd:decimal .

ex:closingStock a owl:DatatypeProperty ;
    rdfs:range xsd:decimal .

ex:grossWeightKg a owl:DatatypeProperty ;
    rdfs:range xsd:decimal .

ex:hasHSCode a owl:ObjectProperty ;
    rdfs:range ex:HSCode .

ex:invoiceNumber a owl:DatatypeProperty ;
    rdfs:range xsd:string .

ex:invoiceDate a owl:DatatypeProperty ;
    rdfs:range xsd:date .

ex:totalAmount a owl:DatatypeProperty ;
    rdfs:range xsd:decimal .

ex:currency a owl:DatatypeProperty ;
    rdfs:range xsd:string .

ex:vatAmount a owl:DatatypeProperty ;
    rdfs:range xsd:decimal .

ex:dutyAmount a owl:DatatypeProperty ;
    rdfs:range xsd:decimal .

